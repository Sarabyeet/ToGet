<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ToGet" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryVariant">@color/primaryColor1</item>
        <item name="colorOnPrimary">@color/primaryTextColor</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/secondaryLightColor</item>
        <item name="colorSecondaryVariant">@color/secondaryColor</item>
        <item name="colorOnSecondary">@color/secondaryTextColor</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorSecondaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="colorOnBackground">@color/primaryLightColor</item>
        <item name="colorOnSurface">@color/secondaryTextColor</item>
        <item name="colorSurface">@color/primaryLightColor</item>
        <item name="android:navigationBarColor">@color/primaryLightColor</item>
        <item name="colorOnPrimarySurface">@color/primaryLightColor</item>
    </style>
</resources>